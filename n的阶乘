#include<stdio.h>
unsigned short int n;   //输入需要进行阶乘的数值；
unsigned short int res; //用于存放计算的结果

int n_factorial()
{
  int i = 0;
  res = 1;
  printf("请输入需要计算阶乘的正整数数字:\n");
  scanf("%d\n",&n);
  if(n>=0)
  {
    for(i=1;i<=n;i++)
    {
      res = res*i;
    }
  }
  else
  {
  printf("请输入正确的正整数！！");
  }
}
